{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "@api/*": [
        "./api/*"
      ],
      "@context/*": [
        "./context/*"
      ],
      "@hook/*": [
        "./hook/*"
      ],
      "@mapper/*": [
        "./mapper/*"
      ],
      "@pages/*": [
        "./pages/*"
      ],
      "@provider/*": [
        "./provide/*r"
      ],
      "@services/*": [
        "./services/*"
      ],
      "@store/*": [
        "./store/*"
      ],
      "@ui/*": [
        "./ui/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@routes/*": [
        "./routes/*"
      ],
      "@routes": [
        "./routes"
      ],
      "@api": [
        "./api"
      ],
      "@context": [
        "./context"
      ],
      "@hook": [
        "./hook"
      ],
      "@mapper": [
        "./mapper"
      ],
      "@pages": [
        "./pages"
      ],
      "@provider": [
        "./provider"
      ],
      "@services": [
        "./services"
      ],
      "@store": [
        "./store"
      ],
      "@ui": [
        "./ui"
      ],
      "@utils": [
        "./utils"
      ]
    }
  },
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
